{
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	  
        ,{
            "path" : "pages/blog/blog",
            "style" : {
				"navigationBarTitleText": "博客"
			}
        }
        ,{
            "path" : "pages/profile/profile",
            "style" : {
				"navigationBarTitleText": "我的"
			}
        }
        ,{
            "path" : "pages/reg/reg",
            "style" : {
				"navigationBarTitleText": "注册"
			}
        }
        ,{
            "path" : "pages/reg/code",
            "style" : {
				"navigationBarTitleText": "验证码"
			}
        } 
        ,{
            "path" : "pages/login/login",
            "style" : {
				"navigationBarTitleText": "登录"
			}
        }
        ,{
            "path" : "pages/edit-userinfo/edit-userinfo",
            "style" : {
				"navigationBarTitleText": "修改信息",
				"app-plus": {
					"bounce":"none",
					"titleNView": {
						"buttons": [
							// 右边
							{
								"color": "#00aaff",
								"colorPressed": "#0087cb",
								"float": "right",
								"fontSize": "36rpx",
								// "fontSrc": "static/font/iconfont.ttf",
								"text": "保存",
								"width": "60px"
							}
						]
					}
				}
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	    "color": "#7A7E83",
	    "selectedColor": "#3cc51f",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [{  
	        "pagePath": "pages/index/index",
	        "iconPath": "static/tabbar/wudao1.png",
	        "selectedIconPath": "static/tabbar/wudao2.png",
	        "text": "舞蹈"
	    }, {
	        "pagePath": "pages/blog/blog",
	        "iconPath": "static/tabbar/boke1.png",
	        "selectedIconPath": "static/tabbar/boke2.png",
	        "text": "博客"
	    },
		{
			"pagePath": "pages/profile/profile",
			"iconPath": "static/tabbar/wo1.png",
			"selectedIconPath": "static/tabbar/wo2.png",
			"text": "我的"
		}] 
	}
}
